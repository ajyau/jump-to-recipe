{
	"manifest_version": 3,
	"name": "Jump to Recipe - RecipeTin Eats",
	"version": "1.0",
	"description": "Jump straight to recipes on recipetineats.com",
	"icons": {
		"16": "images/recipe-book.png",
		"32": "images/recipe-book.png",
		"48": "images/recipe-book.png",
		"128": "images/recipe-book.png"
	},
	"content_scripts": [
		{
			"js": ["scripts/content.js"],
			"matches": ["https://www.recipetineats.com/*"]
		}
	]
}
