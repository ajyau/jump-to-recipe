{
	"manifest_version": 3,
	"name": "Jump to Recipe",
	"version": "1.0",
	"description": "Avoid the fluff at the start of food articles by jumping straight to the recipe.",
	"icons": {
		"16": "images/recipe-book.png",
		"32": "images/recipe-book.png",
		"48": "images/recipe-book.png",
		"128": "images/recipe-book.png"
	},
	"content_scripts": [
		{
			"js": ["scripts/content.js"],
			"matches": [
				"https://developer.chrome.com/docs/extensions/*",
				"https://developer.chrome.com/docs/webstore/*"
			]
		}
	]
}
